<% if (models.length == 0) { %>
  <%= JST['templates/admin/empty_listing']({ id: 'referers', colspan: 3, show: showEmpty }) %>
<% } else { %>
  <% models.each(function(referer) { %>
  <tr>
    <td class="center"><%= referer.get('id') %></td>
    <td><strong><a href="<%= referer.get('url') %>" target="_blank"><%= referer.get('url') %></a></strong></td>
    <td class="center"><%= referer.get('users_count') %></td>
  </tr>
  <% }) %>
<% } %>