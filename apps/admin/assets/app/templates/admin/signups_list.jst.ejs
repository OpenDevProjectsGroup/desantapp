<h2>
  Recently signed users
</h2>

<div id="counter">
  Displaying <var class="signups_counter"><%= signups.length %></var>
  out of <var class="total_count"><%= signups.total_count %></var>.
</div>

<div class="listing_wrapper">
  <table class="listing">
    <thead>
      <th>#</th>
      <th>E-mail</th>
      <th>IP Address</th>
      <th>Created at</th>
    </thead>
    <tbody>
      <% signups.each(function(signup) { %>
      <tr>
        <td class="center"><%= signup.get('id') %></td>
        <td><strong><%= signup.get('email') %></strong></td>
        <td class="center"><%= signup.get('ip_address') %></td>
        <td class="center">
          <time class="timeago" title="<%= signup.get('created_at') %>">
            <%= moment(signup.get('created_at')).format("MMM DD, YYYY") %>
          </time>
        </td>
      </tr>
      <% }) %>
    </tbody>
  </table>
</div>
